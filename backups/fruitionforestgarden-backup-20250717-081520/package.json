  "description": "A blog website for Fruition Forest Garden",
  "main": "src/app.js",
  "scripts": {
    "start": "NODE_ENV=production node src/app.js",
    "dev": "node src/scripts/backup-reminder.js && nodemon src/app.js",
    "backup": "node src/scripts/backup.js",
