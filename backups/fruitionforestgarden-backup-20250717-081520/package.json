    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.10.0",
    "better-sqlite3-session-store": "^0.1.0",
    "compression": "^1.8.0",
    "connect-flash": "^0.1.1",
    "csurf": "^1.10.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^6.0.7",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.17.3",
    "helmet": "^6.1.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.7.0",
